{
  "name": "game-state-example",
  "version": "1.0.0",
  "description": "An example of simple state tracking in a game with Kaboom.",
  "scripts": {
    "start": "parcel src/index.html -p 8000",
    "build": "parcel build --public-url '.' src/index.html --out-dir dist",
    "watch": "parcel watch src/index.html --out-dir dist"
  },
  "author": "Josh Curtiss <josh@curtiss.me>",
  "license": "UNLICENSED",
  "devDependencies": {
    "kaboom": "^0.5.1",
    "parcel-bundler": "1.12.5",
    "parcel-plugin-clean-easy": "^1.0.2",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "typescript": "^4.4.3"
  },
  "alias": {
    "kaboom": "./node_modules/kaboom/dist/kaboom.mjs"
  },
  "parcelCleanPaths": [
    "dist"
  ]
}
